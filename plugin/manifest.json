{
    "manifest_version": 3,
    "name": "console.log***",
    "version": "1.0.0",
    "description": "console.log***",
    "icons": {
        "16": "img/icon/icon.png",
        "48": "img/icon/icon.png",
        "128": "img/icon/icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "img/icon/icon.png",
        "default_title": "CR自动"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/jquery-3.6.js",
                "js/form/temp.js",
                "js/form/index.js",
                "js/form/event.js",
                "js/main.js"
            ],
            "css": [
                "css/wtc_cr.css",
                "css/wtechtec-cr-form.css"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "img/*"
            ],
            "matches": [
                "<all_urls>"
            ],
            "extension_ids": []
        }
    ]
}